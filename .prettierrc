{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 100,
  "bracketSameLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "singleAttributePerLine": true,
	"plugins": ["prettier-plugin-tailwindcss",
      "prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
